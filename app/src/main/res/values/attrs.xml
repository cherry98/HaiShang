<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="com.android.calendar.MultiStateButton"></declare-styleable>

    <declare-styleable name="day">
        <attr name="state_current" format="boolean" />
    </declare-styleable>

    <attr name="calendarViewStyle" format="reference" />

    <declare-styleable name="MonthCalendarView">
        <attr name="month_selected_text_color" format="color" />
